{
  "name": "ReactOsman",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.5.0",
    "@types/jest": "^26.0.19",
    "@types/node": "^12.19.9",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "debounce": "^1.2.0",
   
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "typescript": "^4.1.3",
    "web-vitals": "^0.2.4"
  },

  "build": {
    "appId": "ReactOsman",
    "extends": null,
    "mac": {
      "category": "your.app.category.type"
    },
    "directories":{
      "buildResources": "./dist/*",
      "output":"release"
    },
    "extraMetadata":{
      
      "main" : "./dist/main.js",
      "author":"Osman Aslancan"

    },
    "files":["./dist/*"]
    
  },
  
  "scripts": {
    "build": "webpack",
    "start": "npm run build && electron dist/main.js",
    "electron-build": "npm run build && electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/debounce": "^1.2.0",
    "css-loader": "^5.0.1",
    "electron": "^11.1.0",
    "electron-packager": "^15.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "ts-loader": "^8.0.12",
    "url-loader": "^4.1.1",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0"
  }
}
